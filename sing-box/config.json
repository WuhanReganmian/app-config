{
  "log": {
    "disabled": false,
    "level": "error",
    "timestamp": true
  },
  "dns": {
    "servers": [
      {
        "tag": "dns_proxy",
        "address": "https://1.1.1.1/dns-query",
        "address_resolver": "dns_resolver",
        "strategy": "ipv4_only",
        "detour": "proxy"
      },
      {
        "tag": "dns_direct",
        "address": "https://dns.alidns.com/dns-query",
        "address_resolver": "dns_resolver",
        "strategy": "ipv4_only",
        "detour": "direct"
      },
      {
        "tag": "dns_resolver",
        "address": "223.5.5.5",
        "detour": "direct"
      },
      {
        "tag": "dns_block",
        "address": "rcode://refused"
      }
    ],
    "rules": [
      {
        "outbound": "any",
        "server": "dns_resolver"
      },
      {
        "rule_set": "geosite-geolocation-!cn",
        "server": "dns_proxy"
      },
      {
        "rule_set": [
          "geoip-cn",
          "geosite-cn"
        ],
        "server": "dns_direct"
      },
      {
        "process_name": [
          "com.tencent.mobileqq",
          "com.tencent.mm",
          "com.miui.weather2",
          "com.xiaomi.market",
          "idm.internet.download.manager.plus"
        ],
        "server": "dns_direct"
      }
    ],
    "final": "dns_proxy",
    "strategy": "ipv4_only",
    "disable_cache": false,
    "disable_expire": false
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "tun-in",
      "mtu": 1400,
      "address": [
        "172.18.0.1/30",
        "fdfe:dcba:9876::1/126"
      ],
      "auto_route": true,
      "strict_route": true,
      "stack": "mixed",
      "endpoint_independent_nat": false,
      "sniff": true,
      "sniff_override_destination": false
    }
  ],
  "outbounds": [
    {
      "tag": "proxy",
      "type": "selector",
      "interrupt_exist_connections": false,
      "default": "hy2",
      "outbounds": [
        "hy2",
        "grpc",
        "vision"
      ]
    },
    {
      "tag": "兜底",
      "type": "selector",
      "interrupt_exist_connections": true,
      "default": "direct",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "exclude",
          "keyword": [
            "block|dns"
          ]
        }
      ]
    },
    {
      "tag": "hy2",
      "type": "hysteria2",
      "server": "",
      "server_port": 23456,
      "up_mbps": 50,
      "down_mbps": 300,
      "password": "",
      "tls": {
        "enabled": true,
        "server_name": "aod.itunes.apple.com",
        "insecure": true
      }
    },
    {
      "tag": "grpc",
      "type": "vless",
      "server": "",
      "server_port": 123,
      "uuid": "",
      "tls": {
        "enabled": true,
        "server_name": "aod.itunes.apple.com",
        "utls": {
          "enabled": true,
          "fingerprint": "chrome"
        },
        "reality": {
          "enabled": true,
          "public_key": "",
          "short_id": ""
        }
      },
      "packet_encoding": "xudp",
      "transport": {
        "type": "grpc",
        "service_name": ""
      }
    },
    {
      "tag": "vision",
      "type": "vless",
      "server": "",
      "server_port": 123,
      "uuid": "",
      "flow": "xtls-rprx-vision",
      "tls": {
        "enabled": true,
        "server_name": "aod.itunes.apple.com",
        "utls": {
          "enabled": true,
          "fingerprint": "chrome"
        },
        "reality": {
          "enabled": true,
          "public_key": "",
          "short_id": ""
        }
      },
      "packet_encoding": "xudp"
    },
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "type": "block",
      "tag": "block"
    },
    {
      "type": "dns",
      "tag": "dns-out"
    }
  ],
