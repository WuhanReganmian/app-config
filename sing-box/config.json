            {
                "tag": "dns_proxy",
                "address": "https://1.1.1.1/dns-query",
                "address_resolver": "dns_resolver",
                "strategy": "ipv4_only",
                "detour": "proxy"
            },
            {
                "tag": "dns_direct",
                "address": "https://dns.alidns.com/dns-query",
                "address_resolver": "dns_resolver",
                "strategy": "ipv4_only",
                "detour": "direct"
            },
            {
                "tag": "dns_resolver",
                "address": "223.5.5.5",
                "detour": "direct"
            },
            {
              "tag": "dns_block",
              "address": "rcode://refused"
            }
        ],
        "rules": [
            {
                "outbound": "any",
                "server": "dns_resolver"
            },
            {
                "rule_set": "geosite-geolocation-!cn",
                "server": "dns_proxy"
            },
            {
                "rule_set": ["geoip-cn", "geosite-cn"],
                "server": "dns_direct"
            },
            {
              "process_name": [
                "com.tencent.mobileqq",
                "com.tencent.mm",
                "com.miui.weather2",
                "com.xiaomi.market",
                "idm.internet.download.manager.plus"
              ],
              "server": "dns_direct"
            }
        ],
        "final": "dns_proxy"
    },
